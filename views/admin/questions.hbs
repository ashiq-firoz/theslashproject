<script>
  function stt(){   
   var c = document.getElementById('can')
   var ctx =    c.getContext("2d");
   var img = document.getElementById('output')
   c.height = 300
   c.width = 450
   ctx.drawImage(img, 0, 0);
   document.getElementById('text').innerHTML += '<img src='+c.toDataURL('image/jpeg')+' class="col-12" style="height:max-content;">'
   console.log(c.toDataURL('image/jpeg'))
  }
</script>
